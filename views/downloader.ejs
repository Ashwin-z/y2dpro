  const downloadForm = document.getElementById('download-form');
const urlInput = document.getElementById('txt-url');
const loader = document.querySelector('.loader');
const imageContainer = document.querySelector('.image-container');
const thumbnailImage = document.querySelector('.thumbnail-image');
const titleElement = document.querySelector('.thumb-p');
const tableContainer = document.querySelector('.table-container');
const tableBody = document.getElementById('format-table-body');
const nextLink = document.querySelector('.next-link');

// Validate Instagram URLs and identify their types
function validateInstagramUrl(url) {
    const patterns = {
        post: /^https?:\/\/(?:www\.)?instagram\.com\/p\/([A-Za-z0-9_-]+)/,
        reel: /^https?:\/\/(?:www\.)?instagram\.com\/(?:reel|tv)\/([A-Za-z0-9_-]+)/,
        profilePic: /^https?:\/\/(?:www\.)?instagram\.com\/([A-Za-z0-9_.]+(\/\?img=.+)?)\/?$/
    };

    if (patterns.post.test(url)) {
        return { isValid: true, type: 'post' };
    } else if (patterns.reel.test(url)) {
        return { isValid: false, type: 'reel' };
    } else if (patterns.profilePic.test(url)) {
        return { isValid: false, type: 'profile' };
    }
    
    return { isValid: false, type: 'invalid' };
}

// Show appropriate message based on URL type
function showMessage(urlType) {
    nextLink.style.display = 'block';
    
    if (urlType === 'reel') {
        nextLink.innerHTML = 'Dear user, this is a reel URL. To download the reel, click <a href="/index" style="background: none;">here</a>.';
    } else if (urlType === 'profile') {
        nextLink.innerHTML = 'Dear user, this is a profile picture URL. To view/download the profile picture, click <a href="/mp3" style="background: none;">here</a>.';
    } else {
        nextLink.style.display = 'none';
    }
}

downloadForm.addEventListener('submit', async (e) => {
    e.preventDefault();
    const url = urlInput.value.trim();
    const urlValidation = validateInstagramUrl(url);

    if (!urlValidation.isValid) {
        showMessage(urlValidation.type);
        if (urlValidation.type === 'invalid') {
            // Use SweetAlert2 for invalid URL error
            Swal.fire({
                icon: 'error',
                title: 'Invalid URL',
                text: 'Please enter a valid Instagram URL.',
                confirmButtonText: 'OK'
            });
        }
        return;
    }

    try {
        loader.style.display = 'block';
        imageContainer.style.display = 'none';
        tableContainer.style.display = 'none';
        nextLink.style.display = 'none';

        const response = await fetch('/api/fetch-instagram-post', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
                'Accept': 'application/json'
            },
            body: JSON.stringify({ url })
        });

        const data = await response.json();

        if (!data.success) {
            throw new Error(data.message || 'Failed to fetch data');
        }

        // Update thumbnail and title
        thumbnailImage.src = data.thumbnail || 'default-thumbnail.jpg';
        titleElement.textContent = data.type === 'profile' ? data.username : data.title;

        // Clear and update table
        tableBody.innerHTML = '';
        
        if (data.type === 'profile') {
            addTableRow('Profile Picture', data.thumbnail);
        } else {
            const format = data.mediaType === 'video' ? 'Video' : 'Image';
            addTableRow(format, data.downloadUrl);
        }

        imageContainer.style.display = 'block';
        tableContainer.style.display = 'block';

    } catch (error) {
        // Use SweetAlert2 for any errors during fetch
        Swal.fire({
            icon: 'error',
            title: 'Oops!',
            text: error.message,
            confirmButtonText: 'Try Again'
        });
    } finally {
        loader.style.display = 'none';
    }
});


function addTableRow(format, downloadUrl) {
    const extension = format.toLowerCase() === 'video' ? 'mp4' : 'jpg';
    
    const row = document.createElement('tr');
    row.innerHTML = `
        <td>${format}</td>
        <td>
            <a href="/download?url=${encodeURIComponent(downloadUrl)}&filename=${encodeURIComponent(format)}.${extension}" 
               class="styled-button download-btn" target="_blank">
                <span class="icon"><i class="fas fa-download"></i></span>
                Download
            </a>
        </td>
    `;
    tableBody.appendChild(row);
}





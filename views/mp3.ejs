// Frontend JavaScript

const downloadForm = document.getElementById('download-form');
const urlInput = document.getElementById('txt-url');
const loader = document.getElementById('loader');
const actionButtons = document.getElementById('action-buttons');
const viewPicBtn = document.getElementById('sizebtn');
const downloadBtn = document.getElementById('downloadBtn');
const nextLink = document.querySelector('.next-link');

// Extract username from different URL formats
function extractUsername(input) {
    // Remove trailing slashes and query parameters
    input = input.replace(/\/$/, '')  // Remove trailing slash
                 .split('?')[0]       // Remove query parameters
                 .replace(/^@/, '');   // Remove @ if it exists at the start

    // Regular expressions for different URL patterns
    const patterns = {
        fullUrl: /^https?:\/\/(?:www\.)?instagram\.com\/([A-Za-z0-9_.]*)$/,
        shortUrl: /^(?:www\.)?instagram\.com\/([A-Za-z0-9_.]*)$/,
        directUsername: /^([A-Za-z0-9_.]*)$/
    };

    // Try to match full URL pattern
    let match = input.match(patterns.fullUrl);
    if (match) return match[1];

    // Try to match URL without protocol
    match = input.match(patterns.shortUrl);
    if (match) return match[1];

    // Try to match direct username
    match = input.match(patterns.directUsername);
    if (match) return match[1];

    return null;
}

// Validate Instagram URLs and identify their types
// Validate Instagram URLs and identify their types
function validateInstagramUrl(url) {
    const patterns = {
        profileUrl: /^https?:\/\/(?:www\.)?instagram\.com\/([A-Za-z0-9_.]*)(?:\/)?$/,
        reel: /^https?:\/\/(?:www\.)?instagram\.com\/(?:reel|tv)\/([A-Za-z0-9_-]+)/,
        post: /^https?:\/\/(?:www\.)?instagram\.com\/p\/([A-Za-z0-9_-]+)/
    };

    if (patterns.reel.test(url)) {
        return { isValid: false, type: 'reel' };
    } else if (patterns.post.test(url)) {
        return { isValid: false, type: 'post' };
    } else if (patterns.profileUrl.test(url) || !url.includes('instagram.com')) {
        return { isValid: true, type: 'profile' };
    }

    return { isValid: false, type: 'invalid' };
}

// Show appropriate message based on URL type
function showMessage(urlType) {
    nextLink.style.display = 'block';

    if (urlType === 'reel') {
        nextLink.innerHTML = 'Dear user, this is a reel URL. To download the reel, click <a href="/index" style="background: none;">here</a>.';
    } else if (urlType === 'post') {
        nextLink.innerHTML = 'Dear user, this is a post URL. To download the post, click <a href="/" style="background: none;">here</a>.';
    } else {
        nextLink.style.display = 'none';
    }
}

document.getElementById('start-btn').addEventListener('click', async (e) => {
    e.preventDefault();
    const input = urlInput.value.trim();

    // Hide action buttons initially
    actionButtons.style.display = 'none';

    // Validate URL type first
    const urlValidation = validateInstagramUrl(input);
    if (!urlValidation.isValid) {
        showMessage(urlValidation.type);
        if (urlValidation.type === 'invalid') {
            // Use SweetAlert2 for invalid URL error
            Swal.fire({
                icon: 'error',
                title: 'Invalid URL',
                text: 'Please enter a valid Instagram username or profile URL.',
                confirmButtonText: 'OK'
            });
        }
        return;
    }

    // Extract username from input
    const username = extractUsername(input);
    if (!username) {
        // Use SweetAlert2 for invalid username
        Swal.fire({
            icon: 'error',
            title: 'Invalid Username',
            text: 'Please enter a valid Instagram username or profile URL.',
            confirmButtonText: 'OK'
        });
        return;
    }

    // Show loader while fetching the profile picture
    loader.style.display = 'block';
    nextLink.style.display = 'none';

    try {
        // Fetch the profile picture URL from the server
        const response = await fetch('/profile-pic', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify({ username }),
        });

        const data = await response.json();

        if (data.url) {
            // Show action buttons after fetching the profile picture
            actionButtons.style.display = 'block';

            // Set up View Pic button
            viewPicBtn.onclick = () => window.open(data.url, '_blank');

            // Set up Download button
            downloadBtn.onclick = () => {
                const downloadUrl = `/download-pic?url=${encodeURIComponent(data.url)}`;
                const anchor = document.createElement('a');
                anchor.href = downloadUrl;
                anchor.download = `savereelify.com - ${username}-profile-pic.jpg`;
                anchor.click();
            };
        } else {
            // Use SweetAlert2 for no profile picture found
            Swal.fire({
                icon: 'error',
                title: 'Profile Picture Not Found',
                text: data.error || 'Unable to fetch the profile picture.',
                confirmButtonText: 'OK'
            });
        }
    } catch (error) {
        console.error('Error fetching profile picture:', error);
        // Use SweetAlert2 for errors during fetching
        Swal.fire({
            icon: 'error',
            title: 'Error',
            text: 'An error occurred while fetching the profile picture. Please try again.',
            confirmButtonText: 'OK'
        });
    } finally {
        loader.style.display = 'none';
    }
});




